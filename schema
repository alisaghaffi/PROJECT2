CREATE TABLE movie_tb (
	 
      id int PRIMARY KEY ,
	  imdb_id VARCHAR,
	 
      budget int,
	  title text ,
	  release_date date,
	  revenue bigint,
	  vote_average FLOAT
	 
  );
  
  CREATE TABLE genre_tb (
  	
  id INT PRIMARY KEY  ,
  genre TEXT,
  foreign key(movieId) references movie_tb(id)
);

  
  
CREATE TABLE rating_tb (
     id int PRIMARY KEY  ,
     rating FLOAT,
	foreign key(movieId) references movie_tb(id)
	  );


CREATE TABLE link_tb (
id INT PRIMARY KEY  ,
imdbId int,
tmdbId INT,
foreign key(movieId) references movie_tb(id)
);
